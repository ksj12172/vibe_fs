{
  "buildCommand": "prisma generate --no-engine && next build",
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    },
    "api/**/*.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "PYTHONPATH": "/var/runtime"
    }
  },
  "rewrites": [
    {
      "source": "/api/stock-data/(.*)",
      "destination": "/api/stock-data/[code].py"
    }
  ]
}
